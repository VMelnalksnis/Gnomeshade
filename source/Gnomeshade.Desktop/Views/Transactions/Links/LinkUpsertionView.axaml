<UserControl
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:design="clr-namespace:Gnomeshade.Avalonia.Core.DesignTime;assembly=Gnomeshade.Avalonia.Core"
	xmlns:links="clr-namespace:Gnomeshade.Avalonia.Core.Transactions.Links;assembly=Gnomeshade.Avalonia.Core"
	mc:Ignorable="d" d:DataContext="{x:Static design:DesignTimeData.LinkUpsertionViewModel}"
	x:Class="Gnomeshade.Desktop.Views.Transactions.Links.LinkUpsertionView"
	x:CompileBindings="True"
	x:DataType="links:LinkUpsertionViewModel">

	<StackPanel Orientation="Horizontal">
		<TextBox
			MinWidth="300" Watermark="Link" UseFloatingWatermark="True"
			Text="{Binding UriValue}" TextWrapping="Wrap" />
	</StackPanel>
</UserControl>
